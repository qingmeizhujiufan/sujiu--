<!--logs.wxml-->
<view class="container">
  <swiper class='slider-img' indicator-dots="true" indicator-color="#f5f5f5" indicator-active-color="#3955A2">
    <block wx:for="{{imgUrls}}">
      <swiper-item>
        <image src="{{item}}" class="slide-image" mode='aspectFit' />
      </swiper-item>
    </block>
  </swiper>
</view>
